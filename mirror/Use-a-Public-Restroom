	for(i = 0; i < google_ads.length; ++i) {
		s += '<div class="ad1"' + adText + '>'+ '<h4><a href="' + google_ads[i].url + '"' + adTitle + ' target="_blank">'
		+ google_ads[i].line1 + '</a></h4> '
		+ google_ads[i].line2 + ' ' + google_ads[i].line3 + '<br />' +
		'<a href="' + google_ads[i].url + '" ' + adUrl + ' target="_blank">' + google_ads[i].visible_url + '</a></div>';
